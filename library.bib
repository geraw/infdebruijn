@article{Alur2000,
abstract = {We propose a language, called Charon, for modular specification of$\backslash$ninteracting hybrid systems. For hierarchical description of the system$\backslash$narchitecture, Charon supports building complex agents via the operations$\backslash$nof instantiation, hiding, and parallel composition. For hierarchical$\backslash$ndescription of the behavior of atomic components, Charon supports$\backslash$nbuilding complex modes via the operations of instantiation, scoping,$\backslash$nand encapsulation. Features such as weak preemption, history retention,$\backslash$nand externally defined Java functions, facilitate the description$\backslash$nof complex discrete behavior. Continuous behavior can be specified$\backslash$nusing differential as well as algebraic constraints, and invariants$\backslash$nrestricting the flow spaces, all of which can be declared at various$\backslash$nlevels of the hierarchy. The modular structure of the language is$\backslash$nnot merely syntactic, but can be exploited during analysis. We illustrate$\backslash$nthis aspect by presenting a scheme for modular simulation in which$\backslash$neach mode can be compiled solely based on the locally declared information$\backslash$nto execute its discrete and continuous updates, and furthermore,$\backslash$nsubmodes can integrate at a finer time scale than the enclosing modes.},
author = {Alur, Rajeev and Grosu, Radu and Hur, Yerang and Kumar, Vijay and Lee, Insup},
doi = {10.1007/3-540-46430-1_5},
isbn = {978-3-540-67259-3},
journal = {Hybrid Systems: Computation and Control},
pages = {6--19},
title = {{Modular Specification of Hybrid Systems in Charon}},
