@article{Genero2001,
abstract = {One of the principal objectives of software engineering is to$\backslash$nimprove the quality of software products. It is widely recognised that$\backslash$nthe quality assurance of software products must be guaranteed from the$\backslash$nearly phases of development. As a key artifact produced in the early$\backslash$ndevelopment of object-oriented (OO) information systems (OOISs), class$\backslash$ndiagram quality has a great impact on the quality of the software$\backslash$nproduct which is finally delivered. Hence, class diagram quality is a$\backslash$ncrucial issue that must be evaluated (and improved if necessary) in$\backslash$norder to obtain quality OOISs, which is the main concern of present-day$\backslash$nsoftware development organisations. After a thorough review of the$\backslash$nexisting OO measures that are applicable to class diagrams at a$\backslash$nhigh-level design stage, M. Genero et al. (2000) presented in a set of$\backslash$nmetrics for the structural complexity of class diagrams built using the$\backslash$nUnified Modelling Language (UML). We focus on class diagram structural$\backslash$ncomplexity, an internal quality attribute which we believe could be$\backslash$nclosely correlated with one of the most critical external quality$\backslash$nattributes, such as class diagram maintainability. Since the main goal$\backslash$nof this paper is the empirical validation of those metrics, we present$\backslash$ntwo controlled experiments carried out to corroborate if those metrics$\backslash$nare closed to class diagram maintainability and thus could be used as$\backslash$nearly maintainability indicators. Based on data collected in the$\backslash$nexperiments, we build a prediction model for class diagram$\backslash$nmaintainability using a method for the induction of fuzzy rules},
author = {Genero, M. and Piattini, M. and Jimenez, L.},
doi = {10.1109/SCCC.2001.972637},
isbn = {0-7695-1396-4},
issn = {1522-4902},
journal = {SCCC 2001. 21st International Conference of the Chilean Computer Science Society},
keywords = {class diagram maintainability,empirical,metrics,prediction model,software quality,structural complexity,validation},
title = {{Empirical validation of class diagram complexity metrics}},
year = {2001}
}
