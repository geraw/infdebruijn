isbn = {9780582403857},
issn = {10069313},
journal = {In Practice},
number = {1},
pages = {1--16},
pmid = {20578276},
title = {{An Introduction to the Kalman Filter}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.79.6578{\&}rep=rep1{\&}type=pdf},
